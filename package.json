{
    "name": "prompts-mcp-server",
    "version": "1.0.1",
    "description": "MCP server for managing and providing prompts",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start": "npm run build && node dist/index.js",
        "dev": "tsx --watch src/index.ts",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "keywords": [
        "mcp",
        "prompts",
        "ai",
        "llm"
    ],
    "author": "Eric Wu",
    "license": "MIT",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.0.0",
        "gray-matter": "^4.0.3",
        "chokidar": "^3.5.3"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@vitest/coverage-v8": "^1.0.0",
        "typescript": "^5.0.0",
        "tsx": "^4.0.0",
        "vitest": "^1.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}